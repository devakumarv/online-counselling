<body>

<main class="my-form">
    <div class="cotainer">
        <div class="row justify-content-center">
            <div class="col-md-8">
                    <div class="card">
                        <div class="card-header">Edit Tutor Details | Admin</div>
                        <div class="card-body">
                            <form name="my-form" method="post" action="" >
                                <div class="form-group row">
                                    <label for="full_name" class="col-md-4 col-form-label text-md-right">Name</label>
                                    <div class="col-md-6">
                                        <input type="text" id="full_name" class="form-control" name="name"  >
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">Expertise</label>
                                    <div class="col-md-6">
                                        <input type="text" id="email_address" class="form-control" name="expertise" >
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="user_name" class="col-md-4 col-form-label text-md-right">Fee</label>
                                    <div class="col-md-6">
                                        <input type="text" id="user_name" class="form-control" name="fee">
                                    </div>
                                </div>
                                    <div class="col-md-6 offset-md-4">
                                        <button type="submit" class="btn btn-primary">
                                        Register
                                        </button>
                                    </div>
                                    </form>
                                </div>
                            </div>
                    </div>
            </div>
        </div>

</main>

